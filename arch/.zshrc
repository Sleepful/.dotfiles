#to open new termite tab in the same dir with shift+enter
if [[ $TERM == xterm-termite ]]; then
  . /etc/profile.d/vte.sh
  __vte_osc7
fi

#pretty colors for directories with ls
LS_COLORS=$LS_COLORS:'di=0;35:' ; export LS_COLORS

#partial tab completition coloring
zstyle -e ':completion:*' list-colors 'reply=("${PREFIX:+=(#bi)($PREFIX:t)(?)*==34}:${(s.:.)LS_COLORS}")';

#alias for ranger
alias r='ranger --choosedir=$HOME/.rangerdir; LASTDIR=`cat $HOME/.rangerdir`; cd "$LASTDIR"'

#open dolphin silenced
alias do='dolphin . >/dev/null 2>&1 & '

